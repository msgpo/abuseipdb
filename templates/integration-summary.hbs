{{#if details.isWhitelisted}}
  <span class="integration-summary-tag integration-background-color integration-border-color">
      <span class="acronym integration-text-bold-color">{{block.acronym}}</span>
      <span class="integration-text-color">Is Whitelisted? {{details.isWhitelisted}}</span>
  </span>
{{/if}}
{{#if details.abuseConfidenceScore}}
  <span class="integration-summary-tag integration-background-color integration-border-color">
      <span class="acronym integration-text-bold-color">{{block.acronym}}</span>
      <span class="integration-text-color">Abuse Confidence Score: {{details.abuseConfidenceScore}}</span>
  </span>
{{/if}}
{{#if details.domain}}
  <span class="integration-summary-tag integration-background-color integration-border-color">
      <span class="acronym integration-text-bold-color">{{block.acronym}}</span>
      <span class="integration-text-color">Associated Domain: {{details.domain}}</span>
  </span>
{{/if}}
